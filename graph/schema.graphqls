# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Decimal

type Wallet {
  address: String!
  balance: Decimal!
}

type Query {
  _empty: String
}

input Transfer {
  from_address: String!
  to_address: String!
  amount: Decimal!
}

type Mutation {
  transfer(input: Transfer!): String!
}
